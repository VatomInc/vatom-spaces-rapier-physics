{
  "name": "com.vatom.rapier-physics",
  "version": "1.0.3",
  "description": "A plugin for Vatom Spaces.",
  "main": "src/index.js",
  "author": "jjv360",
  "scripts": {
    "build": "webpack",
    "publish": "npx -p @vatom/cli vatom plugin publish",
    "login": "npx -p @vatom/cli vatom auth",
