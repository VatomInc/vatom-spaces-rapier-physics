    "sideload": "npm run build && npx -p @vatom/cli vatom plugin sideload",
    "sideload2": "npm run build && node ../varius-cli/build plugin sideload",
    "start": "webpack serve"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
