    "babel-loader": "^8.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.21.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.13.2"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "vatom-spaces-plugins": "^1.1.6"
  }
}
